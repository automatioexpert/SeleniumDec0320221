	<test name="FirefoxTest" > <!-- Single / Multiple -->
		<classes> <!-- Single/ Multiple -->
			<class name="com.webdriver.iframes.IframesRunner" />
			<class name="com.webdriver.browserfunction.BrowserFunctionRunner" />
		</classes>
	</test> <!-- Test -->
	<!-- <test name=""> <classes> <class name=""></class> </classes> </test> -->
</suite> <!-- Suite -->
