<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" > <!-- Only once -->
	<listeners>
		<listener class-name="com.webdriver.webelementfunction.SuiteMethods"/>
	</listeners>
	<test name="ChromeTest" > <!-- Single / Multiple -->
		<classes> <!-- Single/ Multiple -->
			<class name="com.webdriver.webelementfunction.WebelementFunctionRunner" />
			<class name="com.webdriver.wait.WebdriverWaitFunctionRunner" />
