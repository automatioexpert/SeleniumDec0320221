<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test name="Test">
		
		<groups>
			<run>
				<!-- It will execute every test case which has a keyword starting with 'Demo'-->
